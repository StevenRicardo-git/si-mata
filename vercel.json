{
    "version": 2,
    "framework": "laravel",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2",
            "memory": 3008,
            "maxDuration": 30
        }
    },
    "buildCommand": "bash build.sh",
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "/storage/$1"
        },
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "^/(.*)",
            "dest": "/api/index.php"
        }
    ]
}